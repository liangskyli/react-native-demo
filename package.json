{
  "name": "DemoProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "update:deps": "pnpm update --interactive --latest",
    "injectBuildTime": "node scripts/inject-build-time.js",
    "android": "react-native run-android",
    "build:android:win-cmd-prod": "pnpm injectBuildTime && cd android && gradlew clean && gradlew assembleRelease",
    "build:android:prod": "pnpm injectBuildTime && cd android && gradlew clean && ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "bundle-ios": "pnpm injectBuildTime && node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js  --platform ios --dev false --bundle-output ./ios/bundle/index.ios.jsbundle --assets-dest ./ios/bundle",
    "lint": "eslint .",
    "start": "pnpm injectBuildTime && react-native start",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@liangskyli/axios-request": "^0.2.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-navigation/stack": "^7.4.8",
    "@shopify/flash-list": "^2.1.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.18",
    "immer": "^10.1.3",
    "lodash": "^4.17.21",
    "lru-cache": "^11.2.2",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.80.2",
    "@dr.pogodin/react-native-fs": "^2.27.1",
    "react-native-bootsplash": "^6.3.10",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-controller": "^1.19.0",
    "react-native-logs": "^5.5.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.6.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@liangskyli/mock": "^6.0.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "^0.80.2",
    "@react-native/eslint-config": "^0.80.2",
    "@react-native/metro-config": "^0.80.2",
    "@react-native/typescript-config": "^0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": "^10"
  }
}
